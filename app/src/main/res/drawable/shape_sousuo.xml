<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"
    android:useLevel="false"
    >
    <!--圆角-->
<corners android:radius="29dp"></corners>
    <!--边距-->
    <padding
        android:bottom="5dp"
        android:top="5dp"
        android:left="20dp"
        android:right="20dp"
        ></padding>
    <!--实心-->
    <solid android:color="@color/white"></solid>
    <!--边框线-->
    <stroke
        android:width="1dp"
        android:color="@color/gray"

        ></stroke>
    <!--大小-->
    <size
        android:width="96dp"
        android:height="13dp"
        ></size>
</shape>